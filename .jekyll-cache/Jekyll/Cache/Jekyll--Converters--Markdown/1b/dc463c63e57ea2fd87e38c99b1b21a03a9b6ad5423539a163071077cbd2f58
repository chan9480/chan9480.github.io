    <span class="n">Derived</span> <span class="n">derived</span><span class="p">(</span><span class="mi">7</span><span class="p">);</span>
	<span class="n">derived</span><span class="p">.</span><span class="n">m_i</span> <span class="o">=</span> <span class="mi">1024</span><span class="p">;</span>   <span class="c1">// Base ì—ì„œ protected ì„ì—ë„ ë¶ˆêµ¬í•˜ê³  ë¬¸ì œ ì—†ë‹¤.</span>

	<span class="k">return</span> <span class="mi">0</span><span class="p">;</span>
