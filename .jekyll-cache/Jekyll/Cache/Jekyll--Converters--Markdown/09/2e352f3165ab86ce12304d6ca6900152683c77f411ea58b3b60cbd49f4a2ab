    def pop(self):
        # top ì¶”ì¶œ
        if self.top == None:        # topì´ None ì´ë©´ return None
            return None
        elif self.top == self.bottom:   # topê³¼ bottomì´ ê°™ë‹¤ë©´(Noneì€ ì•„ë‹˜)
            result = self.top.value     # valueë¥¼ ë¦¬í„´í•˜ê³ , top bottomì„ ë¹„ìš°ì (None)
            self.top =None
            self.bottom = None
            return result
        else :                          # ì´ì œ ì •ìƒì ìœ¼ë¡œ ê¸¸ì´ 1ì´ìƒì˜ ë°í¬ë¼ë©´
            node = self.bottom          # ì´ˆê¸°ê°’ bottomë¶€í„° nextê°€ topì¸ ê³³ê¹Œì§€ nodeë¥¼ ì°¾ì•„ì„œ 
            result = self.top.value     # (ë¦¬í„´ê°’ì€ ì–´ì¨‹ë“  top value)
            while node !=None:          # topì„ ì—…ë°ì´íŠ¸í•´ì£¼ì.
                if node.next == self.top:
                    self.top = node
                node = node.next
            return result</p>
