		<span class="n">txt2</span> <span class="p">=</span> <span class="n">GameObject</span><span class="p">.</span><span class="nf">Find</span> <span class="p">(</span><span class="s">"Text2"</span><span class="p">).</span><span class="n">GetComponent</span><span class="p">&lt;</span><span class="n">Text</span><span class="p">&gt;();</span>

		<span class="nf">SetCommand</span><span class="p">();</span>
	<span class="p">}</span>

	<span class="c1">// SetCommand() ë©”ì†Œë“œë¥¼ í†µí•´ ë²„íŠ¼ì„ ëˆ„ë¥´ë©´ ì–´ë–¤ ë™ì‘ì„ ìˆ˜í–‰í• ì§€ë¥¼ ê° ë²„íŠ¼ì— ë“±ë¡</span>
