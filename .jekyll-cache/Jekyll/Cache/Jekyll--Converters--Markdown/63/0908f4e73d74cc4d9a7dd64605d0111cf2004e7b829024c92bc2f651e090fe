		<span class="k">return</span> <span class="k">this</span><span class="o">-&gt;</span><span class="n">m_cents</span> <span class="o">&lt;</span> <span class="n">c2</span><span class="p">.</span><span class="n">m_cents</span><span class="p">;</span>
